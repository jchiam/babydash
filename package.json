{
  "name": "babydash",
  "version": "1.0.0",
  "description": "Dashboard application for the baby",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "stage": "npm run build && firebase serve",
    "deploy": "npm run lint && npm run build && firebase deploy",
    "lint-ts": "eslint ./ --ext .ts,.tsx,.js,.jsx",
    "lint-scss": "sass-lint src/style/**/*.scss -v -q",
    "lint": "npm run lint-ts && npm run lint-scss"
  },
  "author": "Jonathan Chiam",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "materialize-css": "^1.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/webpack-env": "^1.15.2",
    "autoprefixer": "^9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase": "^7.14.2",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.14.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.20",
    "redux-logger": "^3.0.6",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
